public class FirstLast6ArraysApp {

	public static void main(String[] args) {

	ArrayList<Integer> arrayList = new ArrayList<>();
	arrayList.add(1);
	arrayList.add(2);
	arrayList.add(6);
	
	
	System.out.println(arrayList);
	}

}

