public class FirstLast6ArraysApp {

	public static void main(String[] args) {

		int firstList[] = new int[3];

		firstList[0] = 6;
		firstList[1] = 2;
		firstList[2] = 3;
		
		int last = (firstList.length)-1;
		
		if (firstList[0] == 6 || firstList[last] == 6) {
			System.out.println("True");
		} 
		else {
			System.out.println("False");
		}

